#include <ESP8266WiFi.h>
#include "fsm_ext.h"
#include "control.h"

void relayRELAY_NAMEInit(void) {
    pinMode(RELAY_PIN, OUTPUT);
}

void relayRELAY_NAMEOff(void) {
    digitalWrite(RELAY_PIN, LOW);
    publishRelayState(RELAY_NAME, 0);
}

void relayRELAY_NAMEOn(void) {
    digitalWrite(RELAY_PIN, HIGH);
    publishRelayState(RELAY_NAME, 1);
}
